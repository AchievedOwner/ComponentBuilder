<Project Sdk="Microsoft.NET.Sdk.Razor">
	<Import Project="../../common.props" />
	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Description>分分钟就能创建属于自己的 Blazor 组件库。支持单组件库，多组件库，体系组件库的轻松构建。</Description>
		<PackageTags>Blazor Component Razor</PackageTags>
	</PropertyGroup>
	<ItemGroup>
		<None Include="..\..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="Providers\" />
	</ItemGroup>
	<ItemGroup>
	  <PackageReference Include="OneOf" Version="3.0.223" />
	</ItemGroup>
</Project>
